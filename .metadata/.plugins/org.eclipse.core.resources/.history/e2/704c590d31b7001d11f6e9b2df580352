package Prova;

import java.util.Scanner;
import java.lang.reflect.Array;

public class Teste02 {

	private static final double Segunda = 20.000;
	private static final double Terça = 10.000;
	private static final double Quarta = 15.000;
	private static final double Quinta = 5.000;
	private static final double Sexta = 30.000;

	public static void main(String[] args) {

		double [] FaturamentoDiario = new double [28];
		
		FaturamentoDiario [0]= Segunda;
		FaturamentoDiario [1]= Terça;
		FaturamentoDiario [2]= Quarta;
		FaturamentoDiario [3]= Quinta;
		FaturamentoDiario [4]= Sexta;
		
		
		
		for (int i =0; i < FaturamentoDiario.length; i++) {
			System.out.printf("%d%n" , FaturamentoDiario [i]);
			
		}
		
		
	     
        
	}

}
