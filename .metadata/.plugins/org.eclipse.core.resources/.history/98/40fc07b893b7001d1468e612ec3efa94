package Prova;


public class Teste02 {

	public static void main(String[] args) {

		double [] [] faturamentosDiarios = new double [22][5];
		
		for (int i=0; i < faturamentosDiarios.length; i++) {
			System.out.println("Digite os valores dos faturamentos diarios" + faturamentosDiarios[i]);
		}
			
		for (int j=0; j < faturamentosDiarios.length; j++) {
			System.out.println("Digite o valore do faturamento do mesnsal" + faturamentosDiarios[j]);
		}
			
		
	
	}
}
