package Prova;

import java.util.Scanner;
import java.util.ArrayList;
public class Teste02 {

	public static void main(String[] args) {

		double[][] faturamentosDiarios = new double [28] [5];
		
		int diasSemanas = 0;
		
		faturamentosDiarios [0] [0] = 20.000;
		faturamentosDiarios [0] [1] = 15.000;
		faturamentosDiarios [0] [2] = 5.000;
		faturamentosDiarios [0] [3] = 50.000;
		faturamentosDiarios [0] [4] = 25.000;
		
		for (int i=0; i <faturamentosDiarios.length; i++) {
			
		for (int j=0; j < faturamentosDiarios[i].length; j++) {
			faturamentosDiarios[i] [j] = diasSemanas;
			
		}
			
		}
	
		int maiorValorMes = Integer.MIN_VALUE;
		int menorValorMes = Integer.MAX_VALUE;
		int linha = 0;
		int coluna =0;
		
		for (int i=0; i <faturamentosDiarios.length; i++) {
			
			for (int j=0; j < faturamentosDiarios[i].length; j++) {
				faturamentosDiarios[i] [j] = diasSemanas;
				
				if (faturamentosDiarios [i][j] > maiorValorMes) {
	            	maiorValorMes = (int) faturamentosDiarios [i][j];
	            	linha = i;
	            	coluna = j;
	            }
				
			}
				System.out.println("Maior Valor do mes" + maiorValorMes);
				System.out.println("Menor valor do mes" + menorValorMes);
		}
	}
}
