package Prova;

import java.util.Scanner;

public class tabuada {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		System.out.println("TABUADA DO 5 ");
		int tabuada = sc.nextInt();
		
		
		for (int i = 1; i < 5 ; i++){
			System.out.println("Tabuada" + tabuada);
			i++;
		}
		
		
	}

}
